<div class="row">
  <div class="col-md-6"><h4><small>Welcome back,</small> <%= current_user.name %></h4></div>
  <div class="col-md-6"><span class="pull-right"><h4><small>Last logged in on:</small> <%= formated_date(current_user.last_sign_in_at)%></h4></span></div>

  <div class="col-lg-12">
    <h2>Current Projects:</h2>

    <div class="table-responsive">
      <table class="table table-bordered table-hover ">
        <thead>
        <tr>
          <th>Project#</th>
          <th>Project name</th>
          <th>Customer</th>
          <th>Contact</th>
          <th>Total Logs</th>
          <th>Download Logs</th>
        </tr>
        </thead>
        <tbody>
        <% @projects.each do |project| %>
            <tr>
              <td><%= project.number %></td>
              <td><%= project.name %></td>
              <td><%= project.customer.business_name %></td>
              <td><%= project.contact.name %></td>
              <td><%= "Total log count" %></td>
              <td><%= "Downlod Link" %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div><!-- /.row -->
